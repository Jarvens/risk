<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.scorpion.risk.api.mapper.InterfaceObjMapper">

    <insert id="add" parameterType="com.scorpion.risk.api.entity.InterfaceObj">
        INSERT INTO t_interface(name,type,url,ruleCode,params,paramsType,createTime,updateTime)
        VALUES (#{name},#{type},#{url},#{ruleCode},#{params},#{paramsType},NOW(),NOW())
    </insert>

    <delete id="deleteByPrimaryKey">
        DELETE FROM t_interface WHERE id=#{id}
    </delete>

    <select id="findByPage" resultType="com.scorpion.risk.api.entity.InterfaceObj">
        SELECT * FROM t_interface
    </select>


</mapper>